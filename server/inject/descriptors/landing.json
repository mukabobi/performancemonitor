{
  "type": "object",
  "properties": {
    "isLanding": {
      "type": "identifier",
      "selector": "table",
      "control": "exists",
      "selectIndex": 2
    },
    "linesList": {
      "type": "array",
      "selectorAll": "div > table > tbody > tr:(odd)",
      "items": {
        "type": "object",
        "properties": {
          "linename": {
            "type": "string",
            "selector": "td > div > a",
            "control": "property.innerText"
          },
          "goToDetails": {
            "type": "action",
            "selector": "td > div > a",
            "control": "action.link"
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isLanding"
      ],
      "screens": [
        "landing"
      ]
    }
  ]
}
