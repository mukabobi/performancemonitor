{
  "type": "object",
  "properties": {
    "isLanding": {
      "type": "identifier",
      "selector": "body",
      "control": "exists"
    },
    "linesList": {
      "type": "array",
      "selectorAll": "div",
      "items": {
        "type": "object",
        "properties": {
          "linename": {
            "type": "string",
            "selector": "td > div > a",
            "control": "property.innerText",
            "selectContains": "SMT"
          },
          "goToDetails": {
            "type": "string",
            "selector": "a[href^=\"/Home/PerformanceSMTView?line=\"]",
            "control": "text"
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isLanding"
      ],
      "screens": [
        "landing"
      ]
    }
  ]
}
