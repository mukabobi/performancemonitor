{
  "type": "object",
  "properties": {
    "isLinedata": {
      "type": "identifier",
      "selector": ".mainContainer",
      "control": "exists"
    },
    "linename": {
      "type": "string",
      "selector": ".highlightedShiftText",
      "control": "property.innerHTML"
    },
    "goToLanding": {
      "type": "string",
      "control": "text",
      "selector": "a.navbar-brand"
    },
    "product1": {
      "type": "string",
      "selector": "#partNumberValue1",
      "control": "property.innerText"
    },
    "targetPerHour": {
      "type": "string",
      "selector": "#performanceView-targetValue",
      "control": "property.innerText"
    },
    "hourActualValue": {
      "type": "string",
      "selector": "#performanceView-actualValue",
      "control": "property.innerText"
    },
    "planPerShift": {
      "type": "string",
      "selector": "span#performanceView-shiftTargetValue",
      "control": "property.innerText"
    }
  },
  "connect": [
    {
      "match": [
        "isLinedata"
      ],
      "screens": [
        "linedata"
      ]
    }
  ]
}
