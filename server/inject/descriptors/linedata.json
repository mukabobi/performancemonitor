{
  "type": "object",
  "properties": {
    "isLinedata": {
      "type": "identifier",
      "selector": ".mainContainer",
      "control": "exists"
    },
    "linename": {
      "type": "string",
      "selector": ".highlightedShiftText",
      "control": "property.innerHTML"
    },
    "goToLanding": {
      "type": "action",
      "control": "action.link",
      "selector": "a.navbar-brand"
    }
  },
  "connect": [
    {
      "match": [
        "isLinedata"
      ],
      "screens": [
        "linedata"
      ]
    }
  ]
}
